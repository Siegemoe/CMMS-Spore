{
  "name": "spore-cmms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:security": "jest --testPathPatterns=security",
    "test:api": "jest --testPathPatterns=api",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:e2e": "npx tsx e2e-tests.ts",
    "test:saved": "npx tsx e2e-tests.ts > test-results/SAVED!!! 2>&1 || echo 'Tests completed - see log files'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "prisma:generate": "npx prisma generate",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sentry/nextjs": "^10.23.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "lucide-react": "^0.294.0",
    "next": "15.0.3",
    "next-auth": "^4.24.5",
    "rate-limiter-flexible": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5"
  }
}
